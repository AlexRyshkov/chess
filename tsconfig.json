{
  "compilerOptions": {
    "allowJs": true,
    "module": "ESNext",
    "esModuleInterop": true,
    "target": "ES2021",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "types": ["node", "webpack-env"],
    "baseUrl": "./",
    "paths": {
      "src/*": [
        "./src/*"
      ]
    }
  },
  "exclude": ["dist", "node_modules"],
  "lib": ["es2015"]
}
